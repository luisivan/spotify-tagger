{
  "singleQuote": true,
  "tabWidth": 2,
  "semi": false,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "importOrder": [
    "^.*/lib/.*$",
    "^.*/components/.*$",
    "normalize.css/normalize.css",
    "^.*/pages/(?!.*.css$).*$",
    "^[./](?!.*.css$).*$",
    "^.*(css)$"
  ],
  "importOrderSeparation": false,
  "experimentalBabelParserPluginsList": ["jsx"]
}
